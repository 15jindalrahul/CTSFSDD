<?xml version="1.0"?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
                         "http://java.sun.com/dtd/web-app_2_3.dtd">
<web-app>
 <display-name>Archetype Created Web Application</display-name>
 <servlet>
  <servlet-name>Customer_controller</servlet-name>
  <display-name>Customer_controller</display-name>
  <servlet-class>controller.Customer_controller</servlet-class>
 </servlet>
 <servlet>
  <servlet-name>homeController</servlet-name>
  <display-name>homeController</display-name>
  <servlet-class>controller.homeController</servlet-class>
 </servlet>
 <servlet>
  <servlet-name>find_by_id</servlet-name>
  <display-name>find_by_id</display-name>
  <servlet-class>controller.find_by_id</servlet-class>
 </servlet>
 <servlet>
  <servlet-name>UpdateController</servlet-name>
  <display-name>UpdateController</display-name>
  <servlet-class>controller.UpdateController</servlet-class>
 </servlet>
 <servlet>
 	<servlet-name>deleteController</servlet-name>
 	<display-name>deleteController</display-name>
 	<description></description>
 	<servlet-class>controller.deleteController</servlet-class>
 </servlet>
 <servlet-mapping>
  <servlet-name>Customer_controller</servlet-name>
  <url-pattern>/controller.do</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>homeController</servlet-name>
  <url-pattern>/homecontroller.do</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>find_by_id</servlet-name>
  <url-pattern>/find.do</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>UpdateController</servlet-name>
  <url-pattern>/Updatecontroller.do</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
 	<servlet-name>deleteController</servlet-name>
 	<url-pattern>/deletecontroller.do</url-pattern>
 </servlet-mapping>
</web-app>
